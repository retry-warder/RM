		ИначеЕсли ТипЗнч(Значение) = Тип("ДокументСсылка.РеализацияТоваровУслуг") Тогда
			ИмяПКО = "РеализацияТоваровУслуг";	
		ИначеЕсли ТипЗнч(Значение) = Тип("ДокументСсылка.АктВыполненныхРабот") Тогда
			ИмяПКО = "АктВыполненныхРабот_РеализацияТоваровУслуг";
		ИначеЕсли ТипЗнч(Значение) = Тип("ДокументСсылка.КорректировкаРеализации") Тогда
			ИмяПКО = "КорректировкаРеализации";	
		ИначеЕсли НайденныеСтроки.Количество() &gt; 1 Тогда
			Сообщить("Для объекта " + Значение + " выбрано ИмяПКО " + ИмяПКО + " случайным образом из нескольких возможных ПКО. Проверьте");			
		КонецЕсли;
		Если ТипЗнч(Значение) = Тип("СправочникСсылка.СтруктураПредприятия") Тогда
			Сообщить("Из объекта "+Значение+"("+ТипЗнч(Значение)+")"+" выгружается подразделение. Источник: " + Источник);
		КонецЕсли;
	Иначе
		Отказ = Истина;
		ИмяПКО = "";
	КонецЕсли;
КонецЕсли;

Если Не Отказ И ИмяПКО = "СтруктураПредприятия" Тогда
	ИсходящиеДанные = Новый Структура("Владелец", Источник.Организация);
КонецЕсли;</Текст>
			<Параметры>Источник, Отказ</Параметры>
		</Алгоритм>
	</Алгоритмы>
	<Запросы/>
